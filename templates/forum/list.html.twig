{% extends 'base-back2.html.twig' %}

{% block title %}liste forum{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<h1> liste forum

</h1>

<table border=5>
{%for f in forums%}
<tr>

<td> {{f.titre}}  </td>
<td> {{f.nbrParticipant}}  </td>


<td><a href="{{path('deleteForum',{'id':f.id})}}" > delete </a> </td>
<td><a href="{{path('modifierForum',{'id':f.id})}}" > modifier </a> </td>

</tr>
{% endfor %}
</table>
{% endblock %}
